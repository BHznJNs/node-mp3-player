{
  "name": "@bhznjns/node-mp3-player",
  "version": "1.1.6",
  "description": "A mp3 playing library for Node.js cli.",
  "keywords": ["Node.js", "cli", "mp3", "audio"],
  "homepage": "https://github.com/BHznJNs/node-mp3-player",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "require": "./build/cjs/index.cjs",
      "import": "./build/esm/index.mjs"
    }
  },
  "type": "module",
  "author": "BHznJNs",
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p ./tsconfig.esm.json && tsc -p ./tsconfig.cjs.json",
    "build": "node ./build/build.js"
  },
  "dependencies": {
    "abc": "^0.6.1",
    "axios": "^1.3.2",
    "speaker": "^0.5.4",
    "web-audio-api": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "typescript": "^4.8.4"
  }
}
